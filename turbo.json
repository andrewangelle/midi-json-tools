{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "cache": true,
      "dependsOn": ["^build"],
      "outputs": [] 
    },
    "test": {
      "cache": true,
      "outputs": [],
      "dependsOn": ["^build", "^test"] 
    },
    "format": {
      "cache": false,
      "dependsOn": ["^format"]
    },
    "lint": {
      "cache": false,
      "dependsOn": ["^lint"]
    },
    "@midi-json-tools/json-to-midi#build": {
      "cache": true,
      "outputs": [
        "packages/json-to-midi/dist/**"
      ],
      "dependsOn": ["@midi-json-tools/json-midi-message-encoder#build"]
    },
    "@midi-json-tools/json-to-midi#lint": {
      "cache": true,
      "outputs": [],
      "dependsOn": []
    },
    "@midi-json-tools/json-to-midi#test": {
      "cache": true,
      "outputs": [],
      "dependsOn": ["build"]
    },
    "@midi-json-tools/midi-to-json#build": {
      "cache": true,
      "outputs": [
        "packages/midi-to-json/dist/**"
      ],
      "dependsOn": ["@midi-json-tools/json-midi-message-encoder#build"]
    },
    "@midi-json-tools/midi-to-json#lint": {
      "cache": true,
      "outputs": [],
      "dependsOn": []
    },
    "@midi-json-tools/midi-to-json#test": {
      "cache": true,
      "outputs": [],
      "dependsOn": ["build"]
    },
    "@midi-json-tools/json-midi-message-encoder#build": {
      "cache": true,
      "outputs": [
        "packages/json-midi-message-encoder/dist/**"
      ],
      "dependsOn": []
    },
    "@midi-json-tools/json-midi-message-encoder#lint": {
      "cache": true,
      "outputs": [],
      "dependsOn": []
    },
    "@midi-json-tools/json-midi-message-encoder#test": {
      "cache": true,
      "outputs": [],
      "dependsOn": ["build"]
    }
  }
}